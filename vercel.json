{
  "version": 2,
  "buildCommand": "cd nextjs-app && npm install --legacy-peer-deps && npm run build",
  "installCommand": "cd nextjs-app && npm install --legacy-peer-deps",
  "outputDirectory": "nextjs-app/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
